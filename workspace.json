{
  "version": 2,
  "projects": {
    "signals": {
      "root": "libs/signals",
      "sourceRoot": "libs/signals/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/signals/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/signals"],
          "options": {
            "jestConfig": "libs/signals/jest.config.js",
            "passWithNoTests": true
          }
        },
        "typecheck": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "libs/signals",
            "command": "npx tsc --noEmit -p tsconfig.lib.json && npx tsc --noEmit -p tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
